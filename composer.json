{
  "name": "hegartymaths/groove-api",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dan Keeble",
      "email": "dan@hegartymaths.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Groove\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3",
    "tightenco/collect": "^5.5"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0",
    "satooshi/php-coveralls": "^2.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
